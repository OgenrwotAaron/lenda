{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "module": "commonjs",
    "lib": ["DOM"],
    "outDir": "./dist",
    "resolveJsonModule": true,
    "removeComments": true,
    "alwaysStrict": true,
    "strict": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
  },
  "include": ["src","test"]
}
